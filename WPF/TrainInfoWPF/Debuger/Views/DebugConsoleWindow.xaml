<Window x:Class="TrainInfoWPF.Debuger.DebugConsoleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrainInfoWPF.Debuger"
        mc:Ignorable="d"
        Title="デバッグコンソールウィンドウ" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="MainTextBox" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="Black" Foreground="White" FontSize="14" FontFamily="Yu Gothic Light" TextChanged="MainTextBox_TextChanged" KeyDown="MainTextBox_KeyDown" LostFocus="MainTextBox_LostFocus"/>
    </Grid>
</Window>
